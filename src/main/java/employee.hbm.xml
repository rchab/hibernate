<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.rchab.entity.Employee" table="employee" catalog="hibernate_1">
        <id name="id" type="int">
            <column name="id" />
            <generator class="native" />
        </id>
        <property name="firstName" type="string">
            <column name="first_name" length="20" default="NULL" />
        </property>
        <property name="secondName" type="string" >
            <column name="second_name" length="20" default="NULL" />
        </property>
        <property name="salery" type="int" >
            <column name="salery" default="NULL" />
        </property>
    </class>
</hibernate-mapping>